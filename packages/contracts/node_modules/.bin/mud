#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/darentaylor/Documents/chain/TheOrugginTrail/node_modules/.pnpm/@latticexyz+cli@2.0.0-next.14_google-protobuf@3.21.2/node_modules/@latticexyz/cli/dist/node_modules:/Users/darentaylor/Documents/chain/TheOrugginTrail/node_modules/.pnpm/@latticexyz+cli@2.0.0-next.14_google-protobuf@3.21.2/node_modules/@latticexyz/cli/node_modules:/Users/darentaylor/Documents/chain/TheOrugginTrail/node_modules/.pnpm/@latticexyz+cli@2.0.0-next.14_google-protobuf@3.21.2/node_modules/@latticexyz/node_modules:/Users/darentaylor/Documents/chain/TheOrugginTrail/node_modules/.pnpm/@latticexyz+cli@2.0.0-next.14_google-protobuf@3.21.2/node_modules:/Users/darentaylor/Documents/chain/TheOrugginTrail/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/darentaylor/Documents/chain/TheOrugginTrail/node_modules/.pnpm/@latticexyz+cli@2.0.0-next.14_google-protobuf@3.21.2/node_modules/@latticexyz/cli/dist/node_modules:/Users/darentaylor/Documents/chain/TheOrugginTrail/node_modules/.pnpm/@latticexyz+cli@2.0.0-next.14_google-protobuf@3.21.2/node_modules/@latticexyz/cli/node_modules:/Users/darentaylor/Documents/chain/TheOrugginTrail/node_modules/.pnpm/@latticexyz+cli@2.0.0-next.14_google-protobuf@3.21.2/node_modules/@latticexyz/node_modules:/Users/darentaylor/Documents/chain/TheOrugginTrail/node_modules/.pnpm/@latticexyz+cli@2.0.0-next.14_google-protobuf@3.21.2/node_modules:/Users/darentaylor/Documents/chain/TheOrugginTrail/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@latticexyz/cli/dist/mud.js" "$@"
else
  exec node  "$basedir/../@latticexyz/cli/dist/mud.js" "$@"
fi
