{
  "compilerOptions": {
    "types": ["vite/client", "viem/_types/index", "@metamask/sdk/dist/browser/es/src/index", "lit/index"],
    "strictNullChecks": false,
    "target": "ESNext",
    "useDefineForClassFields": true,
    "experimentalDecorators": false,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "skipLibCheck": true,
    "jsx": "react-jsx"
  },
  "include": ["src"]
}
