#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tims/DATA/BB/o_trail/TheOrugginTrail/on_the_trail/node_modules/.pnpm/wait-port@1.1.0/node_modules/wait-port/bin/node_modules:/Users/tims/DATA/BB/o_trail/TheOrugginTrail/on_the_trail/node_modules/.pnpm/wait-port@1.1.0/node_modules/wait-port/node_modules:/Users/tims/DATA/BB/o_trail/TheOrugginTrail/on_the_trail/node_modules/.pnpm/wait-port@1.1.0/node_modules:/Users/tims/DATA/BB/o_trail/TheOrugginTrail/on_the_trail/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tims/DATA/BB/o_trail/TheOrugginTrail/on_the_trail/node_modules/.pnpm/wait-port@1.1.0/node_modules/wait-port/bin/node_modules:/Users/tims/DATA/BB/o_trail/TheOrugginTrail/on_the_trail/node_modules/.pnpm/wait-port@1.1.0/node_modules/wait-port/node_modules:/Users/tims/DATA/BB/o_trail/TheOrugginTrail/on_the_trail/node_modules/.pnpm/wait-port@1.1.0/node_modules:/Users/tims/DATA/BB/o_trail/TheOrugginTrail/on_the_trail/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/wait-port.js" "$@"
else
  exec node  "$basedir/../../bin/wait-port.js" "$@"
fi
